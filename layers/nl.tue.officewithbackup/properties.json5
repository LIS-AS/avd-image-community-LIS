{
  version: "v2.1",
  name: "nl.tue.officewithbackup",
  description: "Install Office and create backup of user files",
  author: {
    name: "Dirk van Nunen TUe",
    email: "d.j.v.nunen@tue.nl"
  },
  platform_version: "2",
  base_image: {
    type: "PlatformImage",
    publisher: "microsoftwindowsdesktop",
    version: "latest",
    sku: "win10-22h2-avd-m365-g2",
    offer: "office-365"
  },
  network: {
    // HTTP(s) hosts that are whitelisted in the proxy
    // note that the application must be configured to use the proxy or support Windows IE proxy settings
    //
    // list of whitelisted hostname:port combinations. Supports * wildcards
    http_proxy_whitelist: [
      // 'example.com:443', // (port 443 is HTTPS)
      // '*.example.com:*', // (allow any port)
	  "officeclient.microsoft.com:443",
      "odc*.officeapps.live.com:443",
      "ols.officeapps.live.com:443"
    ],
  }
}